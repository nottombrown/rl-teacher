    "max_val = 5000\n",
    "xs = range(max_val)\n",
    "\n",
    "def fn(x):\n",
    "    return 0.25 + 0.75 * (1-decay_rate ** (x/max_val))\n",
    "\n",
    "# Decay to 1% by 100\n",
    "ys = np.vectorize(fn)(xs)\n",
    "plt.plot(xs, ys)\n",
    "plt.show()"
